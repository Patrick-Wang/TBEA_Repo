<?xml version="1.0" encoding="UTF-8"?><components>
    <controller id="E854333332B737FBBB14A55210FC995BIMPORTJSP">
        <response name="components/import_data" type="jsp">
            <map key="importUrl" value="E854333332B737FBBB14A55210FC995BImportClr"/>
        </response>
    </controller>
    <controller id="E854333332B737FBBB14A55210FC995BImportClr">
        <context key="data" value="${request.upfile.asExcel}"/>
        <context key="src" value="${session.account.name} Excel 导入"/>
        <context key="time" value="${calendar.current.secondFormat}"/>
        <callservice id="E854333332B737FBBB14A55210FC995BImportServ"/>
        <response type="json">
            <errorCode>0</errorCode>
            <message>OK</message>
        </response>
    </controller>
    <service id="E854333332B737FBBB14A55210FC995BImportServ" transaction="transactionManager">
        <merge data="${data}" table="exchange_settlement_immediate">
            <set>
                <dwmc ref="0" type="string"/>
                <ywzl ref="1" type="string"/>
                <jbr ref="2" type="date"/>
                <jqhl ref="3" type="double"/>
                <mcbz ref="4" type="string"/>
                <mcje ref="5" type="double"/>
                <mrbz ref="6" type="string"/>
                <mrje ref="7" type="double"/>
                <yh ref="8" type="double"/>
                <_src type="string" value="${src}"/>
                <_time type="date" value="${time}"/>
            </set>
        </merge>
    </service>
</components>
