<?xml version="1.0" encoding="UTF-8"?><components>
    <controller id="financingBankForeignCurrencyCreditImportJSP">
        <response name="components/import_data" type="jsp">
            <map key="importUrl" value="financingBankForeignCurrencyCreditImportClr"/>
        </response>
    </controller>
    <controller id="financingBankForeignCurrencyCreditImportClr">
        <context key="data" value="${request.upfile.asExcel}"/>
        <context key="src" value="${session.account.name} Excel 导入"/>
        <context key="time" value="${calendar.current.secondFormat}"/>
        <callservice id="financingBankForeignCurrencyCreditImportServ"/>
        <response type="json">
            <errorCode>0</errorCode>
            <message>OK</message>
        </response>
    </controller>
    <service id="financingBankForeignCurrencyCreditImportServ" transaction="transactionManager">
        <merge data="${data}" table="financing_bank_foreign_currency_credit">
            <set>
                <bankName ref="0" type="string"/>
                <currency ref="1" type="string"/>
                <authCreditAmount ref="2" type="double"/>
                <usedAmount ref="3" type="double"/>
                <loanBalance ref="4" type="double"/>
                <authCreditExpire ref="5" type="date"/>
                <dwid type="int" value="${dwid}" />
                <_src type="string" value="${src}"/>
                <_time type="date" value="${time}"/>
            </set>
        </merge>
    </service>
</components>
