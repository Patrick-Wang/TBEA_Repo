<components>
 <service id="servWbzlqkEntry" transaction="transactionManager">
  <merge data="${data}" table="zl_wbyclzlwt">
   <set>
    <company_name type="string" ref="0"/>
    <issue_happen_date type="date" ref="1"/>
    <product_type type="string" ref="2"/>
    <production_num type="string" ref="3"/>
    <production_model type="string" ref="4"/>
    <issue_type type="string" ref="5"/>
    <sub_issue_type type="string" ref="6"/>
    <quality_phenomenon type="string" ref="7"/>
    <detail type="string" ref="8"/>
    <material_count type="double" ref="9"/>
    <measurement_units type="string" ref="10"/>
    <suppier type="string" ref="11"/>
    <responsibility_department type="string" ref="12"/>
    <filling_personnel type="string" ref="13"/>
    <product_delivery_date type="date" ref="14"/>
    <failure_subject type="string" ref="15"/>
    <material_treatment_measure type="string" ref="16"/>
    <onsite_treatment_measure type="string" ref="17"/>
    <onsite_treatment_result type="string" ref="18"/>
    <user_unit type="string" ref="19"/>
    <onsite_after_sales type="string" ref="20"/>
    <after_sales_tel type="string" ref="21"/>
    <source type="string" value="${src}"/>
    <time type="date" value="${time}"/>
   </set>
  </merge>
 </service>
 <controller id="wbzlqkImport">
  <context key="data" value="${request.upfile.asExcel}"/>
  <context key="src" value="${session.account.name} Excel 导入"/>
  <context key="time" value="${calendar.current.secondFormat}"/>
  <callservice id="servWbzlqkEntry"/>
  <response type="json">
   <errorCode>0</errorCode>
   <message>OK</message>
  </response>
 </controller>
 <controller id="wbzlqkImportJsp">
  <response type="jsp" name="components/import_data">
   <map key="importUrl" value="wbzlqkImport"/>
   <map key="title" value="外部质量情况导入"/>
  </response>
 </controller>
 <service id="servWbzlqkTrans" transaction="transactionManager">
  <sql>delete from zl_nbyclzlwt where source = ${src}</sql>
  <callservice id="servWbzlqkEntry"/>
 </service>
 <controller id="wbzlqkTransport" cron="0 0 0 3-5 * ?">
  <context key="time" value="${calendar.current.secondFormat}"/>
  <callservice id="servWbzlqkTransLl"/>
  <response type="json">
   <errorCode>0</errorCode>
   <message>OK</message>
  </response>
 </controller>
 <service id="servWbzlqkTransLl" transaction="transLl">
  <context key="src" value="鲁缆数据库收集"/>
  <sql id="data" export="true">select * from zl_wbyclzlwt</sql>
  <callservice id="servWbzlqkTrans"/>
 </service>
 <service id="servWbzlqkTransXl" transaction="transXl">
  <context key="src" value="新缆数据库收集"/>
  <sql id="data" export="true">select * from zl_wbyclzlwt</sql>
  <callservice id="servWbzlqkTrans"/>
 </service>
 <service id="servWbzlqkTransDl" transaction="transDl">
  <context key="src" value="德缆数据库收集"/>
  <sql id="data" export="true">select * from zl_wbyclzlwt</sql>
  <callservice id="servWbzlqkTrans"/>
 </service>
 <service id="servWbzlqkTransSb" transaction="transSb">
  <context key="src" value="沈变数据库收集"/>
  <sql id="data" export="true">select * from zl_wbyclzlwt</sql>
  <callservice id="servWbzlqkTrans"/>
 </service>
 <service id="servWbzlqkTransHb" transaction="transHb">
  <context key="src" value="衡变数据库收集"/>
  <sql id="data" export="true">select * from zl_wbyclzlwt</sql>
  <callservice id="servWbzlqkTrans"/>
 </service>
 <service id="servWbzlqkTransXb" transaction="transXb">
  <context key="src" value="新变数据库收集"/>
  <sql id="data" export="true">select * from zl_wbyclzlwt</sql>
  <callservice id="servWbzlqkTrans"/>
 </service>
</components>
