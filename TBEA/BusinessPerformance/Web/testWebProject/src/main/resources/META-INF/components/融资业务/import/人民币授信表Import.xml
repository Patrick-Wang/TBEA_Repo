<?xml version="1.0" encoding="UTF-8"?><components>
    <controller id="financingBankRMBCreditImportJSP">
        <response name="components/import_data" type="jsp">
            <map key="importUrl" value="financingBankRMBCreditImportClr"/>
        </response>
    </controller>
    <controller id="financingBankRMBCreditImportClr">
        <context key="data" value="${request.upfile.asExcel}"/>
        <context key="src" value="${session.account.name} Excel 导入"/>
        <context key="time" value="${calendar.current.secondFormat}"/>
        <callservice id="financingBankRMBCreditImportServ"/>
        <response type="json">
            <errorCode>0</errorCode>
            <message>OK</message>
        </response>
    </controller>
    <service id="financingBankRMBCreditImportServ" transaction="transactionManager">
        <merge data="${data}" table="financing_bank_RMB_credit">
            <set>
                <bankName ref="0" type="string"/>
                <authCreditAmount ref="1" type="double"/>
                <usedAmount ref="2" type="double"/>
                <loanBalance ref="3" type="double"/>
                <authCreditExpire ref="4" type="date"/>
                <dwid ref="5" type="int"/>
                <_src type="string" value="${src}"/>
                <_time type="date" value="${time}"/>
            </set>
        </merge>
    </service>
</components>
