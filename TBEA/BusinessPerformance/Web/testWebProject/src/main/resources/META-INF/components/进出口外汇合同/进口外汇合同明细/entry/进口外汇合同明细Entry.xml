<?xml version="1.0" encoding="UTF-8"?><components>
    <controller id="991BBF8F796F1CD4451D7CA119E764E5ENTRYJSP">
        <response name="framework/templates/singleDateReport/entry" type="jsp">
            <map key="updateUrl" value="991BBF8F796F1CD4451D7CA119E764E5EntryUpdateClr"/>
            <map key="submitUrl" value="991BBF8F796F1CD4451D7CA119E764E5EntrySubmitClr"/>
        </response>
    </controller>
    <controller id="991BBF8F796F1CD4451D7CA119E764E5EntryUpdateClr">
        <context key="cal" value="${request.date.asCalendar}"/>
        <callservice id="991BBF8F796F1CD4451D7CA119E764E5EntryUpdateServ"/>
        <callcontroller id="991BBF8F796F1CD4451D7CA119E764E5EntryFmtClr"/>
        <formatterServer acceptNullAs="" id="fmtServ" table="${result.matrix}">
            <formatter ref="fmtData"/>
        </formatterServer>
        <response type="json">
            <header type="array">
                <item>
                    <name>单位名称</name>
                    <type>text</type>
                    <readOnly>true</readOnly>
                </item>
                <item>
                    <name>合同编号</name>
                    <type>text</type>
                    <readOnly>true</readOnly>
                </item>
                <item>
                    <name>供货商名称</name>
                    <type>text</type>
                    <readOnly>true</readOnly>
                </item>
                <item>
                    <name>所在国家</name>
                    <type>text</type>
                    <readOnly>true</readOnly>
                </item>
                <item>
                    <name>币种 </name>
                    <type>text</type>
                    <readOnly>true</readOnly>
                </item>
                <item>
                    <name>付款银行</name>
                    <type>text</type>
                    <readOnly>true</readOnly>
                </item>
                <item>
                    <name>合同金额 </name>
                    <readOnly>true</readOnly>
                </item>
                <item>
                    <name>签订合同起始日</name>
                    <type>date</type>
                    <readOnly>true</readOnly>
                </item>
                <item>
                    <name>签订合同汇率</name>
                    <readOnly>true</readOnly>
                </item>
                <item>
                    <name>已付款金额</name>
                </item>
                <item>
                    <name>本月付款金额</name>
                </item>
                <item>
                    <name>付款时间</name>
                    <type>date</type>
                </item>
                <item>
                    <name>款项性质</name>
                    <type>text</type>
                </item>
                <item>
                    <name>付款方式</name>
                    <type>text</type>
                </item>
                <item>
                    <name>待付合同金额</name>
                </item>
                <item>
                    <name>预计${cal.monthAfter[1].month}月付款金额</name>
                </item>
                <item>
                    <name>预计付款时间</name>
                    <type>date</type>
                </item>
                <item>
                    <name>预计${cal.monthAfter[2].month}月付款金额</name>
                </item>
                <item>
                    <name>预计${cal.monthAfter[3].month}月付款金额</name>
                </item>
                <item>
                    <name>预计付款方式</name>
                    <type>text</type>
                </item>
                <item>
                    <name>已操作金额</name>
                </item>
                <item>
                    <name>剩余付款方式</name>
                    <type>text</type>
                </item>
                <item>
                    <name>锁汇价格</name>
                </item>
                <item>
                    <name>交割日</name>
                    <type>date</type>
                </item>
                <item>
                    <name>备注</name>
                    <type>text</type>
                </item>
            </header>
            <shrinkToFit>false</shrinkToFit>
            <pager>none</pager>
            <data>${fmtServ.result}</data>
        </response>
    </controller>
    <controller id="991BBF8F796F1CD4451D7CA119E764E5EntryFmtClr">
        <formatter export="true" id="fmtData">
            <NumberFormatter reservedCount="1">
                <DefaultMatcher cols="9,10,11,15,16,18,19,21,23"/>
            </NumberFormatter>
            <EmptyFormatter/>
        </formatter>
    </controller>

    <service id="991BBF8F796F1CD4451D7CA119E764E5EntryFindServ">
        <sql id="data" export="true"><![CDATA[
        select
            feicd.id tmp0,
            companyName,
            num,
            supplier,
            country,
            currency,
            payingBank,
            amount,
            CONVERT(varchar(20),startDate, 23) tmp7,
            exchangeRate,
            paidAmount,
            paymentAmountTheMonth,
            CONVERT(varchar(20),paymentTime, 23) tmp5,
            moneyType,
            transactionType,
            amountLeft,
            predictPaymentAmountN1,
            CONVERT(varchar(20),predictPaymentDate, 23) tmp10,
            predictPaymentAmountN2,
            predictPaymentAmountN3,
            predictTransactionType,
            operationAmount,
            transactionTypeLeft,
            lockPrice,
            CONVERT(varchar(20),tradeDate, 23) tmp17,
            remark
        from
            foreign_exchange_import_contract_detail feicd,
            foreign_exchange_import_contract feic
        where
            feicd.importContractId = feic.id and
            year = ${cal.year} and
            month = ${cal.month} and
            feic.id = ${item}
        ]]></sql>

    </service>

    <service id="991BBF8F796F1CD4451D7CA119E764E5EntryUpdateServ" transaction="transactionManager">
        <callservice id="991BBF8F796F1CD4451D7CA119E764E5EntryFindServ"></callservice>
        <if test="${data.isEmpty}">
            <sql><![CDATA[
                insert into foreign_exchange_import_contract_detail
                    (importContractId, year, month)
                values
                    (${item}, ${cal.year}, ${cal.month})
            ]]></sql>
            <callservice id="991BBF8F796F1CD4451D7CA119E764E5EntryFindServ"></callservice>
        </if>

        <list id="ids" sql="data" value="0"/>
        <table export="true" id="result" rowIds="ids" table="${data.rmCol[0]}"/>
    </service>
    <controller id="991BBF8F796F1CD4451D7CA119E764E5EntrySubmitClr">
        <context key="cal" value="${request.date.asCalendar}"/>
        <context key="data" value="${request.data.asJsonArray}"/>
        <context key="src" value="${session.account.name} 录入"/>
        <context key="time" value="${calendar.current.secondFormat}"/>
        <callservice id="991BBF8F796F1CD4451D7CA119E764E5EntrySubmitServ"/>
        <response type="json">
            <errorCode>0</errorCode>
            <message>OK</message>
        </response>
    </controller>
    <service id="991BBF8F796F1CD4451D7CA119E764E5EntrySubmitServ" transaction="transactionManager">
        <merge data="${data}" table="foreign_exchange_import_contract_detail">
            <where>
                <id ref="0" type="int"/>
            </where>
            <set>
                <paidAmount ref="10" type="double"/>
                <paymentAmountTheMonth ref="11" type="double"/>
                <paymentTime ref="12" type="date"/>
                <moneyType ref="13" type="string"/>
                <transactionType ref="14" type="string"/>
                <amountLeft ref="15" type="double"/>
                <predictPaymentAmountN1 ref="16" type="double"/>
                <predictPaymentDate ref="17" type="date"/>
                <predictPaymentAmountN2 ref="18" type="double"/>
                <predictPaymentAmountN3 ref="19" type="double"/>
                <predictTransactionType ref="20" type="string"/>
                <operationAmount ref="21" type="double"/>
                <transactionTypeLeft ref="22" type="string"/>
                <lockPrice ref="23" type="double"/>
                <tradeDate ref="24" type="date"/>
                <remark ref="25" type="string"/>
                <_src type="string" value="${src}"/>
                <_time type="date" value="${time}"/>
            </set>
        </merge>
    </service>
</components>
