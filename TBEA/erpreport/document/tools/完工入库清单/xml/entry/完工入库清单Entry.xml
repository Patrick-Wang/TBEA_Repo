<?xml version="1.0" encoding="UTF-8"?><components>
    <controller id="FCCBD80F080E5B9B7AB788E2514E439BENTRYJSP">
        <response name="framework/templates/singleDateReport/entry" type="jsp">
            <map key="updateUrl" value="FCCBD80F080E5B9B7AB788E2514E439BEntryUpdateClr"/>
            <map key="submitUrl" value="FCCBD80F080E5B9B7AB788E2514E439BEntrySubmitClr"/>
        </response>
    </controller>
    <controller id="FCCBD80F080E5B9B7AB788E2514E439BEntryUpdateClr">
        <context key="cal" value="${request.date.asCalendar}"/>
        <callservice id="FCCBD80F080E5B9B7AB788E2514E439BEntryUpdateServ"/>
        <callcontroller id="FCCBD80F080E5B9B7AB788E2514E439BEntryFmtClr"/>
        <formatterServer acceptNullAs="" id="fmtServ" table="${result.matrix}">
            <formatter ref="fmtData"/>
        </formatterServer>
        <response type="json">
            <header type="array">
                <item>
                    <name>序号</name>
                    <type>text</type>
                </item>
                <item>
                    <name>生产号</name>
                    <type>text</type>
                </item>
                <item>
                    <name>产品型号</name>
                    <type>text</type>
                </item>
                <item>
                    <name>图号</name>
                    <type>text</type>
                </item>
                <item>
                    <name>定货单位</name>
                    <type>text</type>
                </item>
                <item>
                    <name>完工时间</name>
                    <type>date</type>
                </item>
                <item>
                    <name>台数</name>
                </item>
                <item>
                    <name>产值</name>
                </item>
                <item>
                    <name>产量</name>
                </item>
                <item>
                    <name>电压等级</name>
                    <type>text</type>
                </item>
                <item>
                    <name>生产中心</name>
                    <type>text</type>
                </item>
                <item>
                    <name>工单号</name>
                    <type>text</type>
                </item>
                <item>
                    <name>物料编码</name>
                    <type>text</type>
                </item>
                <item>
                    <name>物料说明</name>
                    <type>text</type>
                </item>
                <item>
                    <name>完工人</name>
                    <type>text</type>
                </item>
            </header>
            <data>${fmtServ.result}</data>
        </response>
    </controller>
    <controller id="FCCBD80F080E5B9B7AB788E2514E439BEntryFmtClr">
        <formatter export="true" id="fmtData">
            <NumberFormatter reservedCount="0">
                <DefaultMatcher cols="7,8,9"/>
            </NumberFormatter>
            <EmptyFormatter/>
        </formatter>
    </controller>
    <service id="FCCBD80F080E5B9B7AB788E2514E439BEntryUpdateServ" transaction="transactionManager">
        <sql id="data">select 
	id,
	serial,
	source_project_id,
	project_model,
	pic_model,
	order_company,
	CONVERT(varchar(20),transaction_date, 23) tmp5,
	transaction_quantity,
	output_value,
	output,
	electric_level,
	product_center,
	wip_entity_name,
	materiel_code,
	materiel_desc,
	resp_peorson
from CUX_DEVBALANCE_V</sql>
        <list id="ids" sql="data" value="0"/>
        <context key="counter" value="${counterFactory.newCounter}"/>
        <table export="true" id="result" rowIds="ids">
            <list sql="data" type="string" value="${counter.reset[1].val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
            <list sql="data" type="date" value="${counter.next.val}"/>
            <list sql="data" type="int" value="${counter.next.val}"/>
            <list sql="data" type="int" value="${counter.next.val}"/>
            <list sql="data" type="int" value="${counter.next.val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
        </table>
    </service>
    <controller id="FCCBD80F080E5B9B7AB788E2514E439BEntrySubmitClr">
        <context key="cal" value="${request.date.asCalendar}"/>
        <context key="data" value="${request.data.asJsonArray}"/>
        <context key="src" value="${session.account.name} 录入"/>
        <context key="time" value="${calendar.current.secondFormat}"/>
        <callservice id="FCCBD80F080E5B9B7AB788E2514E439BEntrySubmitServ"/>
        <response type="json">
            <errorCode>0</errorCode>
            <message>OK</message>
        </response>
    </controller>
    <service id="FCCBD80F080E5B9B7AB788E2514E439BEntrySubmitServ" transaction="transactionManager">
        <merge data="${data}" table="CUX_DEVBALANCE_V">
            <where>
                <id ref="0" type="int"/>
            </where>
            <set>
                <_src type="string" value="${src}"/>
                <_time type="date" value="${time}"/>
            </set>
        </merge>
    </service>
</components>
