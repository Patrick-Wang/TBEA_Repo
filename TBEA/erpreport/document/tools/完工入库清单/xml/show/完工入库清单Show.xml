<?xml version="1.0" encoding="UTF-8"?><components>
    <controller id="FCCBD80F080E5B9B7AB788E2514E439BSHOWCLR">
        <response name="framework/templates/singleDateReport/show" type="jsp">
            <map key="year" value="${calendar.current.year}"/>
            <map key="month" value="${calendar.current.month}"/>
            <map key="pager" value="pager"/>
            <map key="updateUrl" value="FCCBD80F080E5B9B7AB788E2514E439BUpdateClr"/>
            <map key="exportUrl" value="FCCBD80F080E5B9B7AB788E2514E439BExportClr"/>
        </response>
    </controller>
    <controller id="FCCBD80F080E5B9B7AB788E2514E439BUpdateClr">
        <context key="cal" value="${request.date.asCalendar}"/>
        <context key="pgNum" value="${request.pgNum.asInt}"/>
        <context key="pgSize" value="${request.pgSize.asInt}"/>
        <callservice id="FCCBD80F080E5B9B7AB788E2514E439BShowServ"/>
        <callcontroller id="FCCBD80F080E5B9B7AB788E2514E439BFmtClr"/>
        <formatterServer id="fmtServ" table="${result.matrixNoIds}">
            <formatter ref="fmtData"/>
        </formatterServer>
        <response type="json">
            <header type="array">
                <item>
                    <name>序号</name>
                    <type>text</type>
                </item>
                <item>
                    <name>生产号</name>
                    <type>text</type>
                </item>
                <item>
                    <name>产品型号</name>
                    <type>text</type>
                </item>
                <item>
                    <name>图号</name>
                    <type>text</type>
                </item>
                <item>
                    <name>定货单位</name>
                    <type>text</type>
                </item>
                <item>
                    <name>完工时间</name>
                    <type>date</type>
                </item>
                <item>
                    <name>台数</name>
                </item>
                <item>
                    <name>产值</name>
                </item>
                <item>
                    <name>产量</name>
                </item>
                <item>
                    <name>电压等级</name>
                    <type>text</type>
                </item>
                <item>
                    <name>生产中心</name>
                    <type>text</type>
                </item>
                <item>
                    <name>工单号</name>
                    <type>text</type>
                </item>
                <item>
                    <name>物料编码</name>
                    <type>text</type>
                </item>
                <item>
                    <name>物料说明</name>
                    <type>text</type>
                </item>
                <item>
                    <name>完工人</name>
                    <type>text</type>
                </item>
            </header>
            <data>${fmtServ.result}${dataCount[0]}</data>
            <dateCount/>
        </response>
    </controller>
    <controller id="FCCBD80F080E5B9B7AB788E2514E439BFmtClr">
        <formatter export="true" id="fmtData">
            <NumberFormatter reservedCount="0">
                <DefaultMatcher cols="6,7,8"/>
            </NumberFormatter>
            <EmptyFormatter/>
        </formatter>
    </controller>
    <controller id="FCCBD80F080E5B9B7AB788E2514E439BExportClr">
        <context key="cal" value="${request.date.asCalendar}"/>
        <callservice id="FCCBD80F080E5B9B7AB788E2514E439BShowServ"/>
        <callcontroller id="FCCBD80F080E5B9B7AB788E2514E439BFmtClr"/>
        <ExcelTemplate id="excelTemplate" sheet="完工入库清单"/>
        <list id="title" type="string">序号,生产号,产品型号,图号,定货单位,完工时间,台数,产值,产量,电压等级,生产中心,工单号,物料编码,物料说明,完工人</list>
        <formatter id="excel">
            <ExcelTitleFilter>
                <ExcelTemplate ref="excelTemplate"/>
                <Offset col="0" row="0"/>
                <titles>
                    <title ref="title"/>
                </titles>
            </ExcelTitleFilter>
            <ExcelHeaderCenterFormatter>
                <DefaultMatcher cols="0"/>
                <ExcelTemplate ref="excelTemplate"/>
                <Offset col="0" row="1"/>
            </ExcelHeaderCenterFormatter>
            <ExcelOffsetFormatter>
                <ExcelTemplate ref="excelTemplate"/>
                <Offset col="0" row="1"/>
            </ExcelOffsetFormatter>
        </formatter>
        <formatterServer id="fmtServ" table="${result.matrixNoIds}">
            <formatter ref="fmtData"/>
            <formatter ref="excel"/>
        </formatterServer>
        <response name="${request.date.asString}完工入库清单.xls" ref="excelTemplate" serv="fmtServ" type="excel"/>
    </controller>
    <service id="FCCBD80F080E5B9B7AB788E2514E439BShowServ" transaction="transactionManager">
        <sql export="true" id="dataCount">
select count(*) from CUX_DEVBALANCE_V
</sql>
        <sql id="data" pgNum="${pgNum}" pgSize="${pgSize}">
select 
	id,
	serial,
	source_project_id,
	project_model,
	pic_model,
	order_company,
	CONVERT(varchar(20),transaction_date, 23) tmp5,
	transaction_quantity,
	output_value,
	output,
	electric_level,
	product_center,
	wip_entity_name,
	materiel_code,
	materiel_desc,
	resp_peorson
from CUX_DEVBALANCE_V
</sql>
        <list id="ids" sql="data" value="0"/>
        <context key="counter" value="${counterFactory.newCounter}"/>
        <table export="true" id="result" rowIds="ids">
            <list sql="data" type="string" value="${counter.reset[1].val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
            <list sql="data" type="date" value="${counter.next.val}"/>
            <list sql="data" type="int" value="${counter.next.val}"/>
            <list sql="data" type="int" value="${counter.next.val}"/>
            <list sql="data" type="int" value="${counter.next.val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
            <list sql="data" type="string" value="${counter.next.val}"/>
        </table>
    </service>
</components>
