<?xml version="1.0" encoding="UTF-8"?><components>
    <controller id="FCCBD80F080E5B9B7AB788E2514E439BIMPORTJSP">
        <response name="components/import_data" type="jsp">
            <map key="importUrl" value="FCCBD80F080E5B9B7AB788E2514E439BImportClr"/>
        </response>
    </controller>
    <controller id="FCCBD80F080E5B9B7AB788E2514E439BImportClr">
        <context key="data" value="${request.upfile.asExcel}"/>
        <context key="src" value="${session.account.name} Excel 导入"/>
        <context key="time" value="${calendar.current.secondFormat}"/>
        <callservice id="FCCBD80F080E5B9B7AB788E2514E439BImportServ"/>
        <response type="json">
            <errorCode>0</errorCode>
            <message>OK</message>
        </response>
    </controller>
    <service id="FCCBD80F080E5B9B7AB788E2514E439BImportServ" transaction="transactionManager">
        <merge data="${data}" table="CUX_DEVBALANCE_V">
            <set>
                <_src type="string" value="${src}"/>
                <_time type="date" value="${time}"/>
            </set>
        </merge>
    </service>
</components>
